{
	"Logging": {
		"LogLevel": {
			"Default": "Information",
			"Microsoft.AspNetCore": "Warning"
		}
	},
	"ConnectionStrings": {
		"DefaultConnection": "Server=tcp:investmentportfolio-server.database.windows.net,1433;Initial Catalog=InvestmentPortfolioDB;Persist Security Info=False;User ID={username};Password={password};MultipleActiveResultSets=False;Encrypt=True;TrustServerCertificate=False;Connection Timeout=30;"
	},
	"JwtSettings": {
		"SecretKey": "{generate-strong-key}",
		"Issuer": "https://investmentportfolio-api.azurewebsites.net",
		"Audience": "https://investmentportfolio-frontend.azurewebsites.net",
		"ExpirationMinutes": 1440
	},
	"WcfServices": {
		"AuthService": "https://investmentportfolio-wcf.azurewebsites.net/AuthService.svc",
		"PortfolioService": "https://investmentportfolio-wcf.azurewebsites.net/PortfolioService.svc",
		"AssetService": "https://investmentportfolio-wcf.azurewebsites.net/AssetService.svc",
		"TransactionService": "https://investmentportfolio-wcf.azurewebsites.net/TransactionService.svc",
		"AlertService": "https://investmentportfolio-wcf.azurewebsites.net/AlertService.svc"
	},
	"MarketDataService": {
		"BaseUrl": "https://investmentportfolio-marketdata.azurewebsites.net"
	},
	"AllowedHosts": "*",
	"Cors": {
		"AllowedOrigins": [
			"https://investmentportfolio-frontend.azurewebsites.net",
			"https://your-custom-domain.com"
		]
	}
}